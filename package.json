{
  "name": "node-hhru-api",
  "version": "0.2.12",
  "type": "module",
  "description": "HeadHunter API wrapper for Node.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zoomish/node-hhru-api"
  },
  "keywords": [
    "hh",
    "headhunter",
    "api",
    "headhunter-api",
    "typescript",
    "javascript",
    "sdk",
    "node",
    "nodejs",
    "node-hhru-api",
    "node-hhru",
    "node-hh",
    "hhru"
  ],
  "license": "MIT",
  "author": {
    "name": "Zoomish",
    "url": "https://github.com/Zoomish"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "@Zoomish:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --format=esm --platform=node",
    "build:cjs": "esbuild src/index.ts --bundle --outfile=dist/index.cjs --format=cjs --platform=node",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build && npm run build:cjs && npm run build:types",
    "test": "vitest run tests/all.test.ts",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./common": {
      "import": "./dist/common/common.js",
      "require": "./dist/common/common.cjs",
      "types": "./dist/common/types.d.ts"
    },
    "./applicant": {
      "import": "./dist/applicant/applicant.js",
      "require": "./dist/applicant/applicant.cjs",
      "types": "./dist/applicant/types.d.ts"
    },
    "./employer": {
      "import": "./dist/employer/employer.js",
      "require": "./dist/employer/employer.cjs",
      "types": "./dist/employer/types.d.ts"
    }
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.2",
    "node-fetch": "^3.3.2",
    "prettier": "3.6.2",
    "prettier-plugin-stylex-key-sort": "^1.0.1",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "esbuild": "^0.25.12",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2"
  }
}
