{
  "name": "node-hhru-api",
  "version": "0.0.2",
  "type": "module",
  "description": "HeadHunter API wrapper for Node.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "hh",
    "headhunter",
    "api",
    "typescript"
  ],
  "license": "MIT",
  "author": {
    "name": "Zoomish",
    "url": "https://github.com/Zoomish"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "vitest run tests/all.test.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./common": {
      "import": "./dist/common/common.js",
      "require": "./dist/common/common.cjs",
      "types": "./dist/common/types.d.ts"
    },
    "./applicant": {
      "import": "./dist/applicant/applicant.js",
      "require": "./dist/applicant/applicant.cjs",
      "types": "./dist/applicant/types.d.ts"
    },
    "./employer": {
      "import": "./dist/employer/employer.js",
      "require": "./dist/employer/employer.cjs",
      "types": "./dist/employer/types.d.ts"
    }
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.2",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "prettier": "3.6.2",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2"
  }
}
