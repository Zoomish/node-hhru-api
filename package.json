{
  "name": "node-hhru-api",
  "version": "0.2.4",
  "type": "module",
  "description": "HeadHunter API wrapper for Node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zoomish/node-hhru-api"
  },
  "keywords": [
    "hh",
    "headhunter",
    "api",
    "headhunter-api",
    "typescript",
    "javascript",
    "sdk",
    "node",
    "nodejs",
    "node-hhru-api",
    "node-hhru",
    "node-hh",
    "hhru"
  ],
  "license": "MIT",
  "author": {
    "name": "Zoomish",
    "url": "https://github.com/Zoomish"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "@Zoomish:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "vitest run tests/all.test.ts",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.d.ts",
      "require": "./dist/index.d.ts",
      "types": "./dist/index.d.ts"
    },
    "./common": {
      "import": "./dist/common/common.d.ts",
      "require": "./dist/common/common.d.ts",
      "types": "./dist/common/common.d.ts"
    },
    "./applicant": {
      "import": "./dist/applicant/applicant.d.ts",
      "require": "./dist/applicant/applicant.d.ts",
      "types": "./dist/applicant/applicant.d.ts"
    },
    "./employer": {
      "import": "./dist/employer/employer.d.ts",
      "require": "./dist/employer/employer.d.ts",
      "types": "./dist/employer/employer.d.ts"
    }
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "dotenv": "^17.2.2",
    "node-fetch": "^3.3.2",
    "prettier": "3.6.2",
    "prettier-plugin-stylex-key-sort": "^1.0.1",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2"
  }
}
